void configCmd(WebServer &webServer, WebServer::ConnectionType type, char* urlTail, bool tailComplete) {
  webServer.httpSuccess();
  webServer.print("var config = ['");

  for (int i = 0; i < 4; i++) {
    if (i != 0) {
      webServer.print(".");
    }
    webServer.print(ip[i]);
  }
  
  webServer.print("','");
  for (int i = 0; i < 4; i++) {
    if (i != 0) {
      webServer.print(".");
    }
    webServer.print(gateway[i]);
  }
  
  webServer.print("','");
  for (int i = 0; i < 4; i++) {
    if (i != 0) {
      webServer.print(".");
    }
    webServer.print(subnet[i]);
  }
  
  webServer.print("','");
  for (int i = 0; i < 6; i++) {
    if (i != 0) {
      webServer.print(":");
    }      
    char hex[3];
    hex[0] = (mac[i] >> 4) + 0x30;
    if (hex[0] > 0x39) hex[0] +=7;
    hex[1] = (mac[i] & 0x0f) + 0x30;
    if (hex[1] > 0x39) hex[1] +=7;
    hex[2] = '\0';
    webServer.print(hex);
  }

  webServer.print("'];");
}


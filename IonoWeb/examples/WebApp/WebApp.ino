void setField(byte *ip, char *value, char sep, int base) {
  int p = 0;
  int g = 0;
  for (int c = 0; c < 30; c++) {
    if (value[c] == sep || value[c] == '\0') {
      char n[4];
      int i = p;
      for (; i < c; i++) {
        n[i - p] = value[i];
      }
      n[i - p] = '\0';
      ip[g] = strtoul(n, NULL, base);
      if (value[c] == '\0') {
        break;
      }
      p = c + 1;
      g++;
    }
  }
}

void setup() {
  byte checksum = 7;
  int a = 0;
  for (; a < 4; a++) {
    ip[a] = EEPROM.read(a);
    checksum ^= ip[a];
  }
  for (; a < 8; a++) {
    gateway[a - 4] = EEPROM.read(a);
    checksum ^= gateway[a - 4];
  }
  for (; a < 12; a++) {
    subnet[a - 8] = EEPROM.read(a);
    checksum ^= subnet[a - 8];
  }
  for (; a < 18; a++) {
    mac[a - 12] = EEPROM.read(a);
    checksum ^= mac[a - 12];
  }
  
  if (EEPROM.read(a) != checksum) {
    ip[0] = 192;
    ip[1] = 168;
    ip[2] = 0;
    ip[3] = 100;
    
    gateway[0] = 192;
    gateway[1] = 168;
    gateway[2] = 0;
    gateway[3] = 1;
    
    subnet[0] = 255;
    subnet[1] = 255;
    subnet[2] = 255;
    subnet[3] = 0;
    
    mac[0] = 0;
    mac[1] = 0;
    mac[2] = 0;
    mac[3] = 0;
    mac[4] = 0;
    mac[5] = 0;
  }

  IPAddress ipA(ip[0], ip[1], ip[2], ip[3]);
  IPAddress gatewayA(gateway[0], gateway[1], gateway[2], gateway[3]);
  IPAddress subnetA(subnet[0], subnet[1], subnet[2], subnet[3]);
  Ethernet.begin(mac, ipA, gatewayA, subnetA);
  
  IonoWeb.begin(80);
  IonoWeb.setDefaultCommand(&webPageCmd);
  IonoWeb.addCommand("config", &configCmd);
}

void loop() {
  // Process incoming requests
  IonoWeb.processRequest();
  // Check all the inputs
  Iono.process();
}


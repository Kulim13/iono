void setField(byte *ip, char *value, char sep, int base) {
  int p = 0;
  int g = 0;
  for (int c = 0; c < 30; c++) {
    if (value[c] == sep || value[c] == '\0') {
      char n[4];
      int i = p;
      for (; i < c; i++) {
        n[i - p] = value[i];
      }
      n[i - p] = '\0';
      ip[g] = strtoul(n, NULL, base);
      if (value[c] == '\0') {
        break;
      }
      p = c + 1;
      g++;
    }
  }
}


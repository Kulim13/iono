    }
    for (; a < 22; a++) {
      mac[a - 16] = EEPROM.read(a);
      checksum ^= mac[a - 16];
    }
    for (; a < 38; a+=2) {
      byte l = EEPROM.read(a);
      byte h = EEPROM.read(a + 1);
      pwd[(a - 22) / 2] = (h << 8) + l;
      checksum ^= l;
      checksum ^= h;
    }
    
    if (EEPROM.read(a) != checksum) {
      reset = true;
    }
  }
  
  if (reset) {
    ip[0] = 192;
    ip[1] = 168;
    ip[2] = 0;
    ip[3] = 100;

    dnsa[0] = 192;
    dnsa[1] = 168;
    dnsa[2] = 0;
    dnsa[3] = 1;
    
    gateway[0] = 192;
    gateway[1] = 168;
    gateway[2] = 0;
    gateway[3] = 1;
    
    subnet[0] = 255;
    subnet[1] = 255;
    subnet[2] = 255;
    subnet[3] = 0;
    
    mac[0] = 0;
    mac[1] = 0;
    mac[2] = 0;
    mac[3] = 0;
    mac[4] = 0;
    mac[5] = 0;

    pwd[0] = '\0';
  }

  Ethernet.begin(mac, ip, dnsa, gateway, subnet);
  
  IonoWeb.begin(80);
  WebServer &webServer = IonoWeb.getWebServer();
  webServer.setDefaultCommand(&webPageCmd);
  webServer.addCommand("config", &configCmd);
}

void loop() {
  // Process incoming requests
  IonoWeb.processRequest();
  // Check all the inputs - needed for subscribe
  Iono.process();
}
